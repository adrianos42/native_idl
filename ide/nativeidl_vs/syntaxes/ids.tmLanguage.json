{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "IdlNative",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		},
		{
			"include": "#comments"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [
				{
					"match": "\\b(library|const|true|false|server|client|layer|package)\\b",
					"name": "keyword.control.ids"
				},
				{
					"match": "(?<!\\$)\\b(0x[0-9A-F]*)|([0-9]+\\.?[0-9]*)\\b(?!\\$)",
					"name": "constant.numeric.ids"
				},
				{
					"match": "(?<!\\$)\\b([A-Z][A-Za-z0-9]*)\\b(?!\\$)",
					"name": "entity.name.class.ids"
				}
			]
		},
		"comments": {
			"patterns": [
				{
					"name": "comment.line.ids",
					"match": "(--).*$\n?"
				}
			]
		},
		"strings": {
			"name": "string.quoted.double.ids",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.ids",
					"match": "\\\\."
				}
			]
		}
	},
	"scopeName": "source.ids"
}